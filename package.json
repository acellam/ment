{
  "name": "metvn",
  "appName": "Metvn",
  "cleintVersion": "1.0.0",
  "serverVersion": "1.0.0",
  "version": "1.0.0",
  "description": "A boilerplate for apps built using vue, expressjs, node, mongodb using typescript",
  "scripts": {
    "start": "npm run dev:build && npm run dev:server && npm run dev:client",
    "dev:build": "nodemon --watch webpack.config.js --watch Gruntfile.js --exec grunt",
    "dev:server": "nodemon ./dist/server.js",
    "dev:client": "webpack-dev-server --progress --config webpack.config.js",
    "emit": "tsc",
    "pretest": "tsc && npm run lint && npm run build",
    "test": "npm run test:unit:client && npm run test:unit:server",
    "test:unit:client": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --require ts-node/register 'src/client/**/*.spec.ts'",
    "test:unit:server": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --require ts-node/register 'src/server/**/*.spec.ts'",
    "test:e2e": "tsc -p ./e2e/tsconfig.json && wdio ./e2e/wdio.conf.js",
    "test:e2e:dev": "tsc -p ./e2e/tsconfig.json && set DEBUG=true&& wdio ./e2e/wdio.conf.js",
    "lint": "tslint -c tslint.json '**/*.ts' '**/*.tsx' --exclude '**/node_modules/**'",
    "lint:fix": "npm run lint -- --fix",
    "deploy": " npm run updateProject && npm run deployApp",
    "build": "grunt release"
  },
  "pre-commit": [
    "emit",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mistaguy/metvn"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mistaguy/metvn/issues"
  },
