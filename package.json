{
  "name": "ment",
  "appName": "Ment",
  "cleintVersion": "1.0.0",
  "serverVersion": "1.0.0",
  "version": "1.0.0",
  "description": "A backend boilerplate for apps built using expressjs, node, mongodb using typescript",
  "engines": {
    "node": "11.5.x",
    "npm": "6.4.x"
  },
  "scripts": {
    "start": "npm run watch && dev:server",
    "watch": "nodemon --watch webpack.config.js --watch Gruntfile.js --exec grunt",
    "dev:server": "nodemon ./dist/server.js",
    "prod:server:start": "pm2 start ./dist/server.js",
    "prod:server:stop": "pm2 stop ./dist/server.js",
    "emit": "tsc",
    "pretest": "npm run lint && npm run build:test && tsc",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --opts mocha.opts",
    "test:dev": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --opts mocha.dev.opts",
    "test:unit": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' nyc -r lcov -e .ts -x \"*.spec.ts\" mocha --opts mocha.opts -r ts-node/register src/**/*.spec.ts && nyc report",
    "lint": "tslint -c tslint.json '**/*.ts' --exclude '**/node_modules/**'",
    "lint:fix": "npm run lint -- --fix",
    "build": "grunt developProd",
    "build:release": "grunt release",
    "build:test": "grunt test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "pre-commit": [
    "emit",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mistaguy/ment"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mistaguy/ment/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/classnames": "^2.2.7",
    "@types/core-js": "^2.5.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.11.0",
    "@types/mocha": "^5.2.5",
    "ajv": "^6.6.2",
    "chai": "^4.2.0",
    "check-dependencies": "^1.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^2.6.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "grunt": "^1.0.3",
    "grunt-check-dependencies": "^1.0.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.1",
    "grunt-file-append": "0.0.7",
    "grunt-newer": "^1.3.0",
    "grunt-webpack": "^3.1.1",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "npm-check": "^5.9.0",
    "nyc": "^13.1.0",
    "pre-commit": "^1.2.2",
    "remap-istanbul": "^0.13.0",
    "supertest": "^3.3.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.4",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.16.4",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.119",
    "@types/mongodb": "^3.1.18",
    "@types/mongoose": "^5.3.7",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "cookie": "0.3.1",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^6.0.0",
    "jsonwebtoken": "^8.4.0",
    "jwt-simple": "^0.5.5",
    "moment": "^2.23.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.3",
    "mongoose-data-seed": "^2.0.0",
    "npm": "^6.5.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.0",
    "requirejs": "^2.3.6",
    "swagger-ui-express": "^4.0.2",
    "snyk": "^1.230.5"
  },
  "snyk": true
}
