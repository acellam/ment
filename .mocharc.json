{
  "bail": true,
  "diff": true,
  "exit": true,
  "extension": ["ts"],
  "full-trace": true,
  "package": "./package.json",
  "recursive": true,
  "reporter": "spec",
  "require": [ "ts-node/register", "source-map-support/register" ],
  "slow": 75,
  "spec": "src/**/*.spec.ts",
  "timeout": 10000,
  "ui": "bdd",
  "watch-files": [ "lib/**/*.js", "test/**/*.js" ]
}

